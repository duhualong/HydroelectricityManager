<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <RelativeLayout
        android:id="@id/electricityLightPath"
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="@dimen/my_content_height">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_centerVertical="true"
            android:drawablePadding="@dimen/padding_distance"
            android:text="@string/electricity_light_path"
            style="@style/text_nor"
            android:layout_marginStart="@dimen/middle_left_distance"
            android:layout_marginLeft="@dimen/middle_left_distance"
            android:drawableStart="@drawable/ic_electricity_light_path"
            android:drawableLeft="@drawable/ic_electricity_light_path"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_black_right"
            android:layout_marginEnd="@dimen/right_distance"
            android:layout_marginRight="@dimen/right_distance"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"/>


    </RelativeLayout>
    <View style="@style/divider"/>

    <RelativeLayout
        android:id="@id/faucetFitting"
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="@dimen/my_content_height">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_centerVertical="true"
            android:drawablePadding="@dimen/padding_distance"
            android:text="@string/faucet_fitting"
            style="@style/text_nor"
            android:layout_marginStart="@dimen/middle_left_distance"
            android:layout_marginLeft="@dimen/middle_left_distance"
            android:drawableStart="@drawable/ic_faucet_fitting"
            android:drawableLeft="@drawable/ic_faucet_fitting"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_black_right"
            android:layout_marginEnd="@dimen/right_distance"
            android:layout_marginRight="@dimen/right_distance"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"/>
    </RelativeLayout>
    <View style="@style/divider"/>
    <RelativeLayout
        android:id="@id/bathroomSanitary"
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="@dimen/my_content_height">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_centerVertical="true"
            android:drawablePadding="@dimen/padding_distance"
            android:text="@string/bathroom_sanitary"
            style="@style/text_nor"
            android:layout_marginStart="@dimen/middle_left_distance"
            android:layout_marginLeft="@dimen/middle_left_distance"
            android:drawableStart="@drawable/ic_bathroom_sanitary"
            android:drawableLeft="@drawable/ic_bathroom_sanitary"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_black_right"
            android:layout_marginEnd="@dimen/right_distance"
            android:layout_marginRight="@dimen/right_distance"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"/>
    </RelativeLayout>
    <View style="@style/divider"/>
    <RelativeLayout
        android:id="@id/rushPipe"
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="@dimen/my_content_height">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_centerVertical="true"
            android:drawablePadding="@dimen/padding_distance"
            android:text="@string/rush_pipe"
            style="@style/text_nor"
            android:layout_marginStart="@dimen/middle_left_distance"
            android:layout_marginLeft="@dimen/middle_left_distance"
            android:drawableStart="@drawable/ic_rush_pipe"
            android:drawableLeft="@drawable/ic_rush_pipe"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_black_right"
            android:layout_marginEnd="@dimen/right_distance"
            android:layout_marginRight="@dimen/right_distance"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"/>
    </RelativeLayout>
    <View style="@style/divider"/>
    <RelativeLayout
        android:id="@id/others"
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="@dimen/my_content_height">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_centerVertical="true"
            android:drawablePadding="@dimen/padding_distance"
            android:text="@string/others"
            style="@style/text_nor"
            android:layout_marginStart="@dimen/middle_left_distance"
            android:layout_marginLeft="@dimen/middle_left_distance"
            android:drawableStart="@drawable/ic_others"
            android:drawableLeft="@drawable/ic_others"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_black_right"
            android:layout_marginEnd="@dimen/right_distance"
            android:layout_marginRight="@dimen/right_distance"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"/>
    </RelativeLayout>
</LinearLayout>